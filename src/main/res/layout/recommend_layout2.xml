    xmlns:launcher="http://schemas.android.com/apk/res-auto"
    android:id="@+id/workspace"
    android:layout_width="match_parent"
    android:layout_height="375dip"
    android:paddingRight="30dip"
    launcher:defaultScreen="0"
    launcher:pageSpacing="5px">

    <include
        android:id="@+id/cell1"
        layout="@layout/workspace_screen" />

    <include
        android:id="@+id/cell2"
        layout="@layout/workspace_screen1" />

    <include
        android:id="@+id/cell3"
        layout="@layout/workspace_screen2" />
</com.pkit.launcher.view.HomePagedView>
